% Appendix K: Matlab Program of Multilayer Perceptron
 
% 15k_Appendix_K_mlpKY3_modify.m
% The program is written by Kou-Yuan Huang
 
clear all; 
% I=2+1;% J=3+1;% K=2;
nvectors=4;
ninpdim1=3;
nhid=3;
nhid1=4;
noutdim=2;

i = 0:1:96;
theta = i.*pi/16;
r = 6.5*(104-i)/104;

data(1:97) = r.*sin(theta);
data(2:97)r.*cos(theta);
hold on;
plot(-1*r.*sin(theta),-1*r.*cos(theta),'b*');
 

